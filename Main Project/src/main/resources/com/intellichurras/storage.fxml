<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.collections.*?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.intellichurras.StorageController">
   <children>
        <Button fx:id="backButton" text="Voltar" onAction="#backScreen"/>
        <Label text="Nome do espeto" />
        <TextField fx:id="stickField" PromptText="Espeto..." style="-fx-background-color: #313133; -fx-text-fill: white;"/>
      
        <Label text="Identificador do espeto" />
        <TextField fx:id="idField" PromptText="Identificador..." style="-fx-background-color: #313133; -fx-text-fill: white;"/>

        <Button fx:id="registerButton" text="Registrar" onAction="#registerStick"/>

   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
